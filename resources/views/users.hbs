<div class="container">
        <div class="row">
            <div class="col-md-12">
                {{#if users}}
                    <div class="table-responsive">
                        <table id="user" class="table table-bordered table striped">
                            <thead>
                                <th>First Name</th>
                                <th>Last Name</th>
                                <th>Mob No.</th>
                                <th>Email</th>
                            </thead>

                            <tbody>
                                {{#each users}}
                                    <tr>
                                        <td>{{ first_name }}</td>
                                        <td>{{ last_name }}</td>
                                        <td>{{ mob_no }}</td>
                                        <td>{{ email }}</td>
                                    </tr>
                                {{/each}}
                            </tbody>
                        </table>
                    </div>
                {{/if}}
            </div>
        </div>
    </div>